{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "npm run build && wrangler dev",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test:ai": "npx tsx src/utils/aiRoundTest.ts",
    "test:calibration": "npx tsx scripts/runCalibrationTest.ts",
    "daily-summary": "node scripts/send-daily-summary.js",
    "daily-summary:send": "node scripts/send-daily-summary.js --send",
    "weekly-report": "npx tsx scripts/weekly-report.ts",
    "weekly-report:send": "npx tsx scripts/weekly-report.ts --send",
    "deploy": "npm run build && wrangler deploy"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@react-google-maps/api": "^2.20.7",
    "@supabase/supabase-js": "^2.57.4",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.21",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "nodemailer": "^8.0.1",
    "pdfkit": "^0.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.25.6",
    "@eslint/js": "^9.9.1",
    "@types/pdfkit": "^0.13.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.0.0",
    "wrangler": "^4.69.0"
  }
}
